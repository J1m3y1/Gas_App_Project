      { name: "RaceTrac - Greenwell Springs", lat: 30.5051, lng: -91.0967 },
      { name: "Murphy USA - S Wooddale Blvd", lat: 30.4512, lng: -91.0883 },
      { name: "Murphy Express - Burbank Dr", lat: 30.3783, lng: -91.1037 },
