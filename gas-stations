      { name: "Fuel - Jefferson Hwy", lat: 30.4269, lng: -91.0875 },
      { name: "Costco Gasoline - Dawnadele Ave", lat: 30.4047, lng: -91.0532 },
      { name: "Fuel - S Acadian Thruway", lat: 30.4321, lng: -91.1623 },
