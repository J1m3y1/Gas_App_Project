  <script>
    const map = L.map('map').setView([30.4515, -91.1871], 13);

    L.tileLayer('https://{s}.tile.openstreetmap.org/{z}/{x}/{y}.png', {
      attribution: 'Â© OpenStreetMap contributors'
    }).addTo(map);

    const gasStations = [
      { name: "RaceTrac - Harding Blvd", lat: 30.5235, lng: -91.1682 },
      { name: "Fuel - Staring Ln", lat: 30.3786, lng: -91.1053 },
